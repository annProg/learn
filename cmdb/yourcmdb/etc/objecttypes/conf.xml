				<field name="conf-staff-birth" label="生日" type="text" summaryfield="false" />
			</fieldgroup>
		</fields>
	</object-type>

	<object-type name="方法">
		<fields>
			<fieldgroup name="方法">
				<field name="http-requestmethod" label="方法" type="text" summaryfield="true" />
			</fieldgroup>
		</fields>
	</object-type>

	<object-type name="服务器角色">
		<fields>
			<fieldgroup name="Basic">
				<field name="server-role" label="服务器角色" type="text" summaryfield="true" uniq="true" />
			</fieldgroup>
		</fields>
	</object-type>

	<object-type name="iptype">
		<fields>
			<fieldgroup name="Basic">
				<field name="ip-type" label="IP类型" type="text" summaryfield="true" uniq="true" />
			</fieldgroup>
		</fields>
