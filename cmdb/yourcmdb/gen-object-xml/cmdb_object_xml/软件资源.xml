<group name="软件资源">
	<object-type name="API">
		<fields>
			<fieldgroup name="API基本信息">
				<field name="api-base" label="域名" type="text" uniq="" summaryfield="true" />
				<field name="api-location" label="Location" type="text" uniq="" summaryfield="true" />
				<field name="api-productline" label="产品线" type="text" uniq="" summaryfield="true" />
				<field name="api-function" label="功能" type="text" uniq="" summaryfield="true" />
				<field name="api-comment" label="备注" type="text" uniq="" summaryfield="false" />
			</fieldgroup>
			<fieldgroup name="API监控参数">
				<field name="api-method" label="请求方法" type="text" uniq="" summaryfield="true" />
				<field name="api-params" label="参数拼接" type="text" uniq="" summaryfield="true" />
				<field name="api-data" label="数据提交" type="text" uniq="" summaryfield="true" />
				<field name="api-response" label="期望返回状态码" type="text" uniq="" summaryfield="true" />
				<field name="api-expect" label="期望返回数据" type="text" uniq="" summaryfield="true" />
			</fieldgroup>
		</fields>
	</object-type>
	<object-type name="Docker">
		<fields>
			<fieldgroup name="人员">
				<field name="conf-staff-name" label="姓名" type="text" uniq="" summaryfield="true" />
				<field name="conf-staff-email" label="邮件" type="text" uniq="" summaryfield="false" />
				<field name="conf-staff-phone" label="电话" type="text" uniq="" summaryfield="false" />
				<field name="conf-staff-qq" label="QQ" type="text" uniq="" summaryfield="false" />
				<field name="conf-staff-title" label="职位" type="text" uniq="" summaryfield="false" />
				<field name="conf-staff-birth" label="生日" type="text" uniq="" summaryfield="false" />
			</fieldgroup>
		</fields>
	</object-type>
	<object-type name="Puppet">
		<fields>
			<fieldgroup name="产品线">
				<field name="conf-productline" label="产品线" type="text" uniq="" summaryfield="true" />
			</fieldgroup>
		</fields>
	</object-type>
</group>
