<group name="配置">
	<object-type name="状态">
		<fields>
			<fieldgroup name="状态">
				<field name="conf-status" label="状态" type="text" summaryfield="true" uniq="true" />
			</fieldgroup>
		</fields>
	</object-type>

	<object-type name="产品线">
		<fields>
			<fieldgroup name="产品线">
				<field name="conf-productline-l1" label="一级产品线" type="text" summaryfield="true" uniq="true" />
				<field name="conf-productline-l2" label="二级产品线" type="text" summaryfield="true" />
				<field name="conf-productline-l3" label="三级产品线" type="text" summaryfield="true" />
